ound: white;\r\n  overflow: hidden;\r\n  box-shadow: 0.1px 4px 8px 2px rgba(0, 0, 0.5, 0.5),\r\n    inset 0vw 0vw 1.25vw 0.15vw;\r\n}\r\n\r\n.block .screen .nav {\r\n  display: flex;\r\n\r\n  justify-content: space-between;\r\n}\r\n\r\n.block .screen .nav button {\r\n  border: none;\r\n  background: none;\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n  font: inherit;\r\n  margin: 5px;\r\n}\r\n\r\n.music-container .block {\r\n  margin-top: 20px;\r\n  position: relative;\r\n  width: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.music-container .block .screen {\r\n  position: relative;\r\n  margin: 30px auto 0px auto;\r\n  width: 900px;\r\n  height: 575px;\r\n  background: linear-gradient(135deg, #a5a59b, #d6d5d0);\r\n  border-radius: 10px;\r\n  box-shadow: inset 0px 0px 10px 2px #4d4d4d;\r\n  font-family: 'ChicagoFont', 'Arial';\r\n  font-size: 100%;\r\n  /* color: #484647; */\r\n  color: black;\r\n}\r\n\r\n/* .music-container .block .shape {\r\n  height: 200px;\r\n  width: 320px;\r\n  border-radius: 10%;\r\n  border: 2px solid #000;\r\n  overflow: hidden;\r\n  background-color: #a5a59b;\r\n  box-shadow: inset 0.1px 4px 8px 2px rgba(0, 0, 0, 0.5);\r\n} */\r\n\r\n.music-container .player {\r\n  margin-top: 50px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 100%;\r\n}\r\n\r\n.music-container .main .track-info {\r\n  height: 30px;\r\n  width: 300px;\r\n  overflow: hidden;\r\n  visibility: hidden;\r\n  box-shadow: 0.1px 4px 8px 2px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.music-container .main {\r\n  margin-top: 20px;\r\n  position: relative;\r\n  width: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.music-container .main .player {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin-top: 30px;\r\n}\r\n\r\n.music-container .main .player button {\r\n  height: 55px;\r\n  width: 55px;\r\n  border-radius: 50%;\r\n  font-size: 30px;\r\n  border: none;\r\n  outline: none;\r\n  cursor: pointer;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin: 0 18px;\r\n  background: rgba(0, 0, 0, 0.01);\r\n  box-shadow: inset 0.1px 8px 8px 2px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\nform {\r\n  cursor: pointer;\r\n\r\n  font-family: 'ChicagoFont', 'Arial';\r\n}\r\n\r\n.music-container .main .player .action-btn {\r\n  color: black;\r\n  border: 1px solid rgba(0, 0, 0, 0.08);\r\n  margin: 10%;\r\n}\r\n\r\n.upload {\r\n  background-color: none;\r\n  visibility: hidden;\r\n}\r\n\r\n.upload.active {\r\n  background-color: none;\r\n  visibility: visible;\r\n  transition: 0.3s;\r\n}\r\n\r\n.tracklist {\r\n  list-style: none;\r\n  position: absolute;\r\n  bottom: 0;\r\n  z-index: 1;\r\n  width: 100%;\r\n  height: 100%;\r\n  /* background: white; */\r\n  max-height: 0px;\r\n  overflow: hidden;\r\n  transition: 0.5s;\r\n  display: flex;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.tracklist.active {\r\n  position: absolute;\r\n  bottom: 0;\r\n  z-index: 10;\r\n  width: 100%;\r\n  height: 95%;\r\n  margin-top: 10px;\r\n  max-height: 0px;\r\n  overflow: scroll;\r\n  transition: 1s;\r\n  display: flex;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.tracklist li {\r\n  position: relative;\r\n  width: 80%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  border-radius: 10px;\r\n  transition: 0.5s;\r\n  cursor: pointer;\r\n  margin: 10px;\r\n  padding: 12px 12px 20px 12px;\r\n  /* border: 1px solid rgba(0, 0, 0, 0.08); */\r\n  border: 1px solid black;\r\n  /* box-shadow: 0.1px 8px 8px 2px rgba(0, 0, 0, 0.1); */\r\n  /* background: linear-gradient(135deg, #a5a59b, #d6d5d0); */\r\n  border-radius: 10px;\r\n  /* box-shadow: inset 0px 0px 10px 2px #4d4d4d; */\r\n  font-family: 'ChicagoFont', 'Arial';\r\n  font-size: 100%;\r\n  color: black;\r\n}\r\n\r\n.tracklist.active::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n\r\naudio::-webkit-media-controls-panel {\r\n  background-color: #bebeb9;\r\n  opacity: 100%;\r\n}\r\n\r\n.music-container .active {\r\n  max-height: 100vh;\r\n  padding: 100px 0 0 0;\r\n}\r\n"},"lineCount":222}},"error":null,"hash":"f4888d34b91429d94a3a8ee8bddd2d87","cacheData":{"env":{}}}